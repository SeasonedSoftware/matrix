    "express": "^4.16.4",
    "hashmap": "^2.3.0",
    "jquery": "^3.4.1",
    "md5": "^2.2.1",
    "nodemon": "^1.18.10",
    "popper.js": "^1.15.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
