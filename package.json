{
  "name": "matrix",
  "version": "1.0.0",
  "description": "The first open virtual office",
  "main": "app.server.js",
  "scripts": {
    "start-server": "nodemon --exec babel-node app.server.js",
    "webpack-server": "webpack --watch --config webpack.development.js",
    "build": "webpack --mode production --config webpack.production.js",
    "start": "npm-run-all -p start-server webpack-server",
